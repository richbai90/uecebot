{
  "name": "fullstack-typescript",
  "version": "1.1.1",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "16.11.0"
  },
  "scripts": {
    "test": "tsc ./src/utils/helper/courseOverlaps.ts && node ./src/utils/helper/courseOverlaps.js",
    "clean": "rm -rf dist",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "type-check-client": "tsc -p ./src/client/tsconfig.json",
    "build": "tsc -b ./",
    "start": "node --unhandled-rejections=strict index.js",
    "prestart": "npm run clean && npm run build"
  },
  "keywords": [
    "typescript",
    "node"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gilamran/fullstack-typescript.git"
  },
  "license": "MIT",
  "author": "Gil Amran",
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/client/setupEnzyme.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "<rootDir>/src/client/tsconfig-for-tests.json"
      }
    }
  },
  "dependencies": {
    "@elastic/app-search-node": "^7.13.0",
    "core-js": "^3.12.1",
    "discord.js": "^13.3.1",
    "dotenv": "^8.6.0",
    "luxon": "^1.27.0",
    "natural": "^5.0.3",
    "node-fetch": "^2.6.6",
    "node-html-parser": "^5.1.0",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/luxon": "^1.26.5",
    "@types/natural": "^2.1.1",
    "@types/node": "^14.14.22",
    "@types/node-fetch": "^3.0.3",
    "@types/request": "^2.48.5",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "dts-gen": "^0.6.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.5.4"
  }
}
